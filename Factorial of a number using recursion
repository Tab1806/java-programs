import java.util.Scanner;
class factr
{
	public static void main(String[] args)
	{
		int num;
		double f=1;
		System.out.println("Enter an integer:");
		Scanner s=new Scanner(System.in);
		num=s.nextInt();
		f=fact(num);
		System.out.println("Factorial of"+num+" is "+f);
	}
	static int fact(int n)
	{
		if(n==0)
		{
			return 1;
		}
		else
		{
			return(n*fact(n-1));
		}
	}
}
