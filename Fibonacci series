import java.util.Scanner;
class fibonacii
{
	public static void main(String[] args)
	{
		int a=0,b=0,c=1;
		int n,i;
		System.out.println("Enter any number:");
		Scanner s=new Scanner(System.in);
		n=s.nextInt();
		System.out.println("Fibonacii Series................");
		for (i=1;i<=n;i++)
		{
			a=b;
			b=c;
			c=a+b;
			System.out.print(a+ ",  ");
		}

	}
}
