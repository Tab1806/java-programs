import java.util.Scanner;
class palindrome
{
	public static void main(String[] args)
	{
		int a,b,no,temp=0;
		System.out.println("Enter any number:");
		Scanner s=new Scanner(System.in);
		no=s.nextInt();
		b=no;
		while(no>0)
		{
			a=no%10;
			no=no/10;
			temp=temp*10+a;
		}
		if(temp==b)
		{
			System.out.println(b+" is palindrome");
		}
		else
		{
			System.out.println(b+" is not palindrome");
		}
	}
}
